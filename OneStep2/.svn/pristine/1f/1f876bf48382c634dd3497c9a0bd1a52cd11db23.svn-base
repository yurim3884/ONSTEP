<%@ page language="java" contentType="text/html; charset=UTF-8" %>
  <%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
    <%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %>
      <%@ taglib uri="http://www.springframework.org/tags/form" prefix="form" %>
        <div class="sub-header-container">
          <header class="header navbar navbar-expand-sm">
            <ul class="navbar-nav flex-row">
              <li>
                <div class="page-header">
                  <nav class="breadcrumb-one" aria-label="breadcrumb">
                    <ol class="breadcrumb">
                      <li class="breadcrumb-item"><a href="javascript:void(0);">Forms</a></li>
                    </ol>
                  </nav>
                </div>
              </li>
            </ul>
            <ul class="navbar-nav d-flex align-center ml-auto right-side-filter">
              <li class="">
                <p class="current-time" id="currentTime"></p>
                <p class="current-date" id="currentDate"></p>
              </li>
            </ul>
          </header>
        </div>

        <div class="layout-px-spacing">
          <div class="layout-top-spacing mb-2">
            <div class="col-md-12">
              <div class="row">
                <div class="container p-0">
                  <div class="row layout-top-spacing">
                    <div class="col-lg-12 layout-spacing">
                      <div class="statbox widget box box-shadow">

                        <form action="/company/insert" method="post" enctype="multipart/form-data">

                          <div class="widget-content widget-content-area">
                            <div class="w-100">
                              <div class="mt-2">
                                <div class="form-group row">
                                  <label class="col-3">공고 제목</label>
                                  <div class="col-9">
                                    <input class="form-control form-control-solid" type="text" name="annoTitle"
                                      value="1" required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">공고 내용</label>
                                  <div class="col-9">
                                    <input class="form-control form-control-solid" type="text" name="annoContent"
                                      value="1" required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">공고 타입</label>
                                  <div class="col-9">
                                    <input class="form-control form-control-solid" type="text" name="annoType" value="1"
                                      required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">담당자 이름</label>
                                  <div class="col-9">
                                    <input class="form-control form-control-solid" type="text" name="annoManager"
                                      value="1" required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">담당자 전화번호</label>
                                  <div class="col-9">
                                    <div class="input-group input-group-solid">
                                      <div class="input-group-prepend">
                                        <span class="input-group-text">
                                          <i class="la la-phone"></i>
                                        </span>
                                      </div>
                                      <input type="text" class="form-control form-control-solid" placeholder="Phone"
                                        value="1" name="annoTel" required>
                                    </div>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">담당자 이메일</label>
                                  <div class="col-9">
                                    <div class="input-group input-group-solid">
                                      <div class="input-group-prepend">
                                        <span class="input-group-text">
                                          <i class="la la-at"></i>
                                        </span>
                                      </div>
                                      <input type="text" class="form-control form-control-solid" placeholder="Email"
                                        value="1" name="annoEmail" required>
                                    </div>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">근무 환경</label>
                                  <div class="col-9">
                                    <input class="form-control form-control-solid" type="text" name="annoWork" value="1"
                                      required>
                                  </div>
                                </div>

                                <div class="form-group row">
                                  <label class="col-3">연령 제한</label>
                                  <div class="col-9">
                                    <input class="form-control" type="number" value="20" id="example-number-input"
                                      name="annoAge" required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">수습기간 (개월)</label>
                                  <div class="col-9">
                                    <input class="form-control" type="number" value="6" id="example-number-input"
                                      name="annoProbation" required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">급여 (만원)</label>
                                  <div class="col-9">
                                    <input class="form-control" type="number" value="3000" id="example-number-input"
                                      name="annoSalary" required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">업종</label>
                                  <div class="col-9">
                                    <input class="form-control form-control-solid" type="text" name="industryCode"
                                      value="1" required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">모집분야</label>
                                  <div class="col-9">
                                    <input class="form-control form-control-solid" type="text" name="annoMb" value="1"
                                      required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">모집인원</label>
                                  <div class="col-9">
                                    <input class="form-control" type="number" value="1" id="example-number-input"
                                      name="annoMem" required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">경력</label>
                                  <div class="col-9">
                                    <!--                                                             <input class="form-control form-control-solid" type="text" -->
                                    <!--                                                                  name="annoCareer"> -->
                                    <select class="form-control form-control-sm" id="exampleSelects" name="annoCareer"
                                      required>
                                      <option>신입</option>
                                      <option>경력</option>
                                      <option>신입 경력</option>
                                    </select>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">담당업무</label>
                                  <div class="col-9">
                                    <input class="form-control form-control-solid" type="text" name="annoDm" required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">근무부서</label>
                                  <div class="col-9">
                                    <input class="form-control form-control-solid" type="text" name="annoGb" value="1"
                                      required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">지원조건</label>
                                  <div class="col-9">
                                    <input class="form-control form-control-solid" type="text" name="annoJo" value="1"
                                      required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">우대사항</label>
                                  <div class="col-9">
                                    <input class="form-control form-control-solid" type="text" name="annoUd" value="1"
                                      required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">성별</label>
                                  <div class="col-9">
                                    <select class="form-control form-control-sm" id="exampleSelects" name="annoGender"
                                      required>
                                      <option>남성</option>
                                      <option>여성</option>
                                      <option>상관없음</option>
                                    </select>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">근무요일</label>
                                  <div class="col-9">
                                    <select class="form-control form-control-sm" id="exampleSelects" name="annoDay"
                                      required>
                                      <option> 주 6일</option>
                                      <option> 주 5일</option>
                                    </select>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">근무 시간</label>
                                  <div class="col-9">
                                    <input class="form-control" type="number" value="8" id="example-number-input"
                                      name="annoTime" required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">지역</label>
                                  <div class="col-9">
                                    <input class="form-control form-control-solid" type="text" name="annoWd" required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">학력</label>
                                  <div class="col-9">
                                    <!--                                                             <input class="form-control form-control-solid" type="text" -->
                                    <!--                                                                  name="annoStu"> -->
                                    <select class="form-control form-control-sm" id="exampleSelects" name="annoStu"
                                      required>
                                      <option> 석/박사</option>
                                      <option> 대학교 (4년제)</option>
                                      <option> 대학 (2~3년제)</option>
                                      <option> 고등학교</option>
                                    </select>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">고용형태</label>
                                  <div class="col-9">
                                    <!--                                                             <input class="form-control form-control-solid" type="text" -->
                                    <!--                                                                  name="annoStaly"> -->
                                    <select class="form-control form-control-sm" id="exampleSelects" name="annoStaly"
                                      required>
                                      <option> 인턴</option>
                                      <option> 정규직</option>
                                      <option> 임원</option>
                                    </select>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">자기소개서 항목</label>
                                  <div class="col-9">
                                    <input class="form-control form-control-solid" type="text" name="annoIntro"
                                      required>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">공고 시작 날짜</label>
                                  <div class="col-9">
                                    <input class="form-control" type="date" value="2011-08-19" id="example-date-input"
                                      name="annoStartDate" required>
                                    <%-- <fmt:formatDate value="2011-08-19" pattern="yyyy-MM-dd" var="annoStartDate" />
                                    --%>
                                  </div>
                                </div>
                                <div class="form-group row">
                                  <label class="col-3">공고 종료 날짜</label>
                                  <div class="col-9">
                                    <input class="form-control" type="date" value="2011-08-19" id="example-date-input"
                                      name="annoEndDate" required>
                                  </div>
                                </div>
                                
                                <div class="form-group row">
                                  <label class="col-3">키워드</label>
                                  <div class="col-9" >
                                    <select class="form-control multiple" id="exampleSelects" name="keyword" multiple="multiple"
                                      required>
                                    <c:forEach items="${keyword }" var="keyword">
<!--                                     <div class="form-group"> -->
<%--                                         <input onclick="f_cntCk(this)" class="inp-cbx" type="checkbox" value="${keyword.annoKw }" name="keyword">${keyword.annoKwname}                                  --%>
<!--                                     </div> -->
											<option> ${keyword.annoKwname } </option>
                                      </c:forEach>
                                      </select>
                                    </div>
                                    </div>

                                  <div class="input-group row">
                                    <label class="col-3">공고 이미지</label>
                                    <div class="col-9 ">
                                      <div class="widget-content widget-content-area">
                                        <div class="form-group row">
                                          <div class="col-lg-12 col-md-12 col-sm-12">
                                            <div class="attached-files">
                                              <img id="image-preview" width="320">
                                            </div>
                                            <label for="file-upload" class="custom-file-upload mb-0">
                                              <a title="Attach a file" class="mr-2 pointer text-primary">
                                                <i class="las la-paperclip font-20"></i>
                                                <span class="font-17">Click here to attach an image</span>
                                              </a>
                                            </label>
                                            <input id="file-upload" name="attAnno" type="file" accept="image/*"
                                              style="display:none;">
                                          </div>
                                        </div>
                                      </div>
                                    </div>
                                  </div>
                                </div>
                              </div>
                            </div>
                            <div class="widget-footer text-right">
                              <button type="submit" class="btn btn-primary mr-2" id="submit">Submit</button>
                              <button type="reset" class="btn btn-outline-primary"
                                onclick="location.href = '/company/main' ">Cancel</button>
                            </div>
                        </form>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        
        <script type="text/javascript">
        	
      
        
        </script>

      