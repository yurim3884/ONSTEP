    border-radius: 4px;
    color: #4165a7;
    font-size: 13px;
    font-weight: 500;
    cursor: pointer;
    transition: all ease 0.4s;
}
.email-right .email-right-top .email-options-container .email-pagination{
    display: inline-block;
    padding: 7px 14px;
    border-radius: 4px;
    color: #4165a7;
    font-size: 13px;
    font-weight: 500;
    transition: all ease 0.4s;
}
.email-right .email-right-top .email-options-container .dropdown-menu a {
