}
.email-right .email-right-top .email-options-container {
    width: 50%;
    display: flex;
    align-items: center;
    justify-content: flex-end;
}
.email-right .email-right-top .email-options-container .email-pagination:hover{
    display: inline-block;
    background: #f4f8ff;
