     <insert id="insert" parameterType="calVO" useGeneratedKeys="true">
      <selectKey order="BEFORE" keyProperty="calId" resultType="int">
         select seq_calid.nextval from dual
      </selectKey>
      insert into cal(
         cal_id
         , cal_title
         , cal_content
         , cal_start
         , cal_end
         , mem_id
         , anno_id
         , id
         , label
         )values(
           #{calId}
         , #{calTitle}
         , #{calContent}
		 , #{calStart}
		 , #{calEnd}
         , #{memId}
         , #{annoId} 
         , #{id}
         , #{label}
         )
   </insert>
   
   
   
   <update id="update" parameterType="calVO" >
   		update cal
   		   set 
	           cal_title = #{calTitle}
	         , cal_content =#{calContent}
	         , cal_start = #{calStart}
	         , cal_end = #{calEnd}
	         , anno_id = #{annoId}
	         , label = #{label}
	    where cal_id = #{calId} 
   </update>
   
   
   <delete id="del" parameterType="calVO">
   		delete from cal
   		 where cal_id =#{calId}
   </delete>
   
   
   <select id="selectfullList" resultType="annoVO">
 	  select
              a.com_Id
            , a.anno_startDate
            , a.anno_endDate
            , B.COMPANY_NAME
       from ANNOUNCEMENT a
       left outer join company b on (a.com_id = b.COMPANY_ID)
   </select>
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
   
