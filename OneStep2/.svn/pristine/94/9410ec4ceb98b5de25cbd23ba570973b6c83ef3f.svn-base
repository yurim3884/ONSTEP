/* TYPE 2 */
.type-2 .dragula {
    padding: 15px 15px;
    background: #f0f6ff;
    border-radius: 8px;
    min-height: 250px;
    height: 90%;
}
.type-2 .dragula div.media {
    background-color: #fff!important;
    padding: 0;
    margin: 0;
}
.type-2 .dragula div.media,
body.gu-unselectable div.media.drag-type-2.gu-mirror {
    background-color: #fff;
    border-radius: 6px;
    border: 1px solid #e0e6ed;
    padding: 14px 26px;
    margin-bottom: 10px;
}
.type-2 #right-rm-spill div.media.ex-moved {
    background-color: #e74c3c !important;
}
.type-2 .dragula div.media:hover {
    border: 1px solid #f1f2f3;
    border-radius: 4px;
}
.type-2 .dragula .media img,
body.gu-unselectable .media.drag-type-2.gu-mirror img {
    width: 45px;
    border-radius: 10%;
    margin-right: 17px;
    height: 45px;
}
.type-2 .dragula .media .media-body,
body.gu-unselectable .media.drag-type-2.gu-mirror .media-body {
    align-self: center;
}
.type-2 .dragula .media .media-body h6,
body.gu-unselectable .media.drag-type-2.gu-mirror .media-body h6 {
    color: #3b3f5c;
    font-weight: 600;
    font-size: 15px;
    margin-top: 0;
    margin-bottom: 0;
}
.type-2 #right-rm-spill div.media.ex-moved .media-body h6,
body.gu-unselectable .media.drag-type-2.gu-mirror.ex-moved .media-body h6 {
    color: #ffffff;
    font-weight: 600;
    font-size: 15px;
    margin-top: 0;
    margin-bottom: 0;
}
.type-2 .dragula .media .media-body p,
body.gu-unselectable .media.drag-type-2.gu-mirror .media-body p {
    margin-top: 0;
    margin-bottom: 0;
}
.type-2 #right-rm-spill div.media.ex-moved .media-body p,
body.gu-unselectable .media.drag-type-2.gu-mirror.ex-moved .media-body p {
    color: #ffffff;
}
