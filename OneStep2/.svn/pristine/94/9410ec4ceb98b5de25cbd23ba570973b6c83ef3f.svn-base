.dragula > div,
.gu-mirror {
    margin: 10px 0px;
    padding: 10px;
    transition: opacity 0.4s ease-in-out;
}
.dragula > div, .dragula > p {
    cursor: move;
    cursor: grab;
    cursor: -moz-grab;
    cursor: -webkit-grab;
}
.gu-mirror {
    cursor: grabbing;
    cursor: -moz-grabbing;
    cursor: -webkit-grabbing;
}
.dragula .ex-moved {
    background-color: #e74c3c;
}
.dragula.ex-over {
    background-color: rgba(255, 255, 255, 0.3);
}
#left-lovehandles > div,
#right-lovehandles > div {
    cursor: initial;
}
.image-thing {
    margin: 20px 0;
    display: block;
    text-align: center;
}
.slack-join {
    position: absolute;
    font-weight: normal;
    font-size: 14px;
    right: 10px;
    top: 50%;
    margin-top: -8px;
    line-height: 16px;
}
/* TYPE 1 */
.type-1 .dragula {
    padding: 5px 15px;
    background: #f0f6ff;
    border-radius: 8px;
    min-height: 400px;
}
.type-1 .dragula .media,
body.gu-unselectable .media.drag-type-1 {
    background-color: #fff;
    border-radius: 6px;
    border: 1px solid #e0e6ed;
    padding: 14px 26px;
}
.type-1 .dragula .media img,
body.gu-unselectable .media.drag-type-1 img {
    width: 50px;
    /* border-radius: 50%; */
    margin-right: 18px;
    height: 50px;
}
.type-1 .dragula .media .media-body,
body.gu-unselectable .media.drag-type-1 .media-body {
    align-self: center;
}
.type-1 .dragula .media .media-body h6,
body.gu-unselectable .media.drag-type-1 .media-body h6 {
    color: #3b3f5c;
    font-weight: 600;
    font-size: 15px;
    margin-top: 0;
    margin-bottom: 0;
}
.type-1 .dragula .media .media-body p,
body.gu-unselectable .media.drag-type-1 .media-body p {
    color: #888ea8;
    font-weight: 600;
    margin-bottom: 0;
    font-size: 12px;
}
/* DRAG ANYTHING */
.drag-any .dragula {
    padding: 5px 15px;
    background: #f0f6ff;
    border-radius: 8px;
    min-height: 400px;
    height: 100%;
}
