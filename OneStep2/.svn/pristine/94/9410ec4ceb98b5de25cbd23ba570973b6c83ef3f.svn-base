}
/************************************** Media Query **************************************/
@media(max-width: 575px) {
    .type-1 .dragula .media img, body.gu-unselectable .media.drag-type-1 img {
        width: 40px;
        margin-right: 0px;
        height: 40px;
        margin-bottom: 15px;
    }
    .type-1 .dragula .media .media-body h6, body.gu-unselectable .media.drag-type-1 .media-body h6 {
        margin-bottom: 5px;
    }
    .type-1 .dragula .media .media-body p, body.gu-unselectable .media.drag-type-1 .media-body p {
        margin-bottom: 5px;
    }
    .type-1 .col-sm-6, .drag-any .col-sm-6, .type-2 .col-md-6, .type-3 .col-md-6 {
        width: 50%;
        padding: 0px 5px;
    }
}
